entity Message {
	text String
    createdDate LocalDate
}

entity Participant {

}

entity Room {
	name String
    createdDate LocalDate
}

entity Profile {
    phoneNumber String
}

relationship OneToOne {
    Profile to User with jpaDerivedIdentifier
}


relationship OneToMany {
    Room to Message
    Room to Participant
}

relationship ManyToOne {
    Participant to User
    Message to User
}
